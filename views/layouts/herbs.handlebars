<div class="herbContainer">
  <div id="banner"><img src="/foodbuddyDEMO.jpg"></div>
  <div class="row">
    <div class="col-lg-12 text-left">
      <div>
        <h1>Herbs</h1>
        <br />
        {{#each herb}}
        <ul>
          <li>{{ FoodItem }}<strong> Purchased: </strong>{{ moment TimestampQ "from" dfrom}}</li>
        </ul>
        {{/each}}
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
  function getHerbs() {
    $.ajax({ url: "/api/data/herb", method: "GET" })
      .then(function (theData) {
        console.log("Hello World" + theData);
      });
  }
</script>